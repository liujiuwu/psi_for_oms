<?xml version="1.0" encoding="utf-8"?>

<resources>
	<declare-styleable name="com.wooboo.adlib_android.WoobooAdView">
		<attr name="backgroundColor" format="color" /> <!-- 广告背景颜色 默认为黑色 -->
		<attr name="textColor" format="color" /><!-- 广告文字颜色 默认为白色 -->
		<attr name="testing" format="boolean" />
		<!--
			广告是否处于测试状态 为保证您的收益，请你在上传或者发布apk之前把该参数设置成false，默认为false
		-->
		<attr name="refreshInterval" format="integer" /><!-- 广告请求时间间隔 -->
	</declare-styleable>

	<!-- 要显示农历生日的语言（亚洲国家或地区） -->
	<string-array name="lunarLocales">
		<item>zh</item>
		<item>zh_CN</item>
		<item>zh_TW</item>
		<item>ja</item>
		<item>ko</item>
		<item>ko_KR</item>
		<item>ja_JP</item>
	</string-array>

	<!-- 十二生肖 -->
	<string-array name="animals">
		<item>鼠</item>
		<item>牛</item>
		<item>虎</item>
		<item>兔</item>
		<item>龙</item>
		<item>蛇</item>
		<item>马</item>
		<item>羊</item>
		<item>猴</item>
		<item>鸡</item>
		<item>狗</item>
		<item>猪</item>
	</string-array>
</resources>
